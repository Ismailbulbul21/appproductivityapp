{
  "expo": {
    "name": "Qorsheyn",
    "slug": "productivityapp",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "productivityapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6d28d9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.qorsheyn",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.productivityapp.abaabulka",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6d28d9"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#6d28d9"
        }
      ],
      "@react-native-community/datetimepicker",
      "./plugins/withFocusBlocking.js"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "426862d3-6a1f-4445-8568-22d573821bb8"
      }
    }
  }
}